{
  "name": "vagrant",
  "version": "1.0.0",
  "description": "Настроим Vagrant-окружение c помощью [Ansible Provisioning](https://www.vagrantup.com/docs/provisioning/ansible) и запустим внутри него Fastify-проект.",
  "main": "app.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -w -l info -P app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "fastify": "^3.0.0",
    "fastify-plugin": "^3.0.0",
    "fastify-autoload": "^3.3.1",
    "fastify-sensible": "^3.1.0",
    "fastify-cli": "^2.13.0"
  },
  "devDependencies": {
    "tap": "^15.0.9"
  }
}